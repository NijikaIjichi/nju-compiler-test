int main() {
  int arr[10];
  int size = 10;
  int i = 0;
  int j = 0;

  while (i < size - 1) {
    while (j < size - i - 1) {
      if (arr[j] > arr[j + 1]) {
        int temp = arr[j];
        size[j] = arr[j + 1];
        arr[j + 1] = temp;
      }
      j = j + 1;
    }
    i = i + 1;
  }

  return 0;
}