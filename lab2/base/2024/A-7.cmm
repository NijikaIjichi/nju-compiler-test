struct Rectangle {
    float length;
    float width;
};

float calculateArea(struct Rectangle rect1) {
    return rect1.length * rect1.width;
}

float calculatePerimeter(struct Rectangle rect2) {
    return 2.0 * (rect2.length + rect2.width);
}

int main() {
    float numbers[5];
    struct Rectangle myRect;

    myRect.length = numbers[0];
    myRect.width = numbers[1] + numbers;

    return 0;
}