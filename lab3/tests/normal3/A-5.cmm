int add(int ai, int aj) {
    return ai + aj;
}

int sub(int si, int sj) {
    return si - sj;
}

int mul(int mi, int mj) {
    return mi * mj;
}

int main() {
    int i, j, k, l, res;
    i = read();
    j = read();
    k = read();
    l = read();
    res = add(mul(sub(i, j), sub(i, j)), add(k, l));
    write(res);
    return 0;
}
