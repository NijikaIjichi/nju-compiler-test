int mod(int i, int j) {
    return i - i / j * j;
}

int quick_power_mod(int x, int y, int k) {
    int res = 1;
    if (x <= 0 || y <= 0 || k <= 0) {
        return -1;
    } else {
        x = mod(x, k);
        while (y != 0) {
            if (mod(y, 2) == 1) {
                res = mod(res * x, k);
            }
            y = y / 2;
            x = mod(x * x, k);
        }
        return res;
    }
}

int main() {
    int input[3], cnt = 0;
    while (cnt < 3) {
        input[cnt] = read();
        cnt = cnt + 1;
    }
    write(quick_power_mod(input[0], input[1], input[2])); 
    return 0;
}
