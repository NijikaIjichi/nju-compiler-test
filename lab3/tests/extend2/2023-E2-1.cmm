int main() {
  int mat[4][4];
  int i,j,k;
  i = 0;
  while(i < 4){
    j = 0;
    while(j < 4){
      mat[i][j] = 999;
      j = j + 1;
    }
    i = i + 1;
  }

  i = 0;
  while(i < 4){
    mat[i][i] = 0;
    i = i + 1;
  }
  mat[0][1] = 3;
  mat[0][3] = 5;
  mat[1][0] = 2;
  mat[1][3] = 4;
  mat[2][1] = 1;
  mat[3][2] = 2;

  k = 0;
  while(k < 4){
    i = 0;
    while(i < 4){
      j = 0;
      while(j < 4){
        if(mat[i][k] + mat[k][j] < mat[i][j]){
          mat[i][j] = mat[i][k] + mat[k][j];
        }
        j = j + 1;
      }
      i = i + 1;
    }
    k = k + 1;
  }

  write(mat[0][3]);
  write(mat[1][2]);
  write(mat[2][1]);
  write(mat[3][0]);

  return 0;
}